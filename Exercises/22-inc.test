({
  name: 'inc',
  length: [30, 70],
  tests: [
    [{ n: 0 }, { n: 1 }],
    [{ n: 1 }, { n: 2 }],
    [{ n: -1 }, { n: 0 }],
    [{ n: 10000 }, { n: 10001 }],
  ],
  test: inc => {
    const a = { n: 5 };
    const b = inc(a);
    if (a !== b) throw new Error('Result and argument should be equal');
    if (typeof b !== 'object') throw new Error('Result should be Object');
  }
})
